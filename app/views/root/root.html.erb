<script type="text/javascript">
  $(function () {
    var currentUser = <%= self.current_user.to_json.html_safe %>;
    var notebooksData = <%= render(:file => 'notebooks/index', :formats => [:json], :locals => { :notebooks => self.current_user.notebooks.includes(:notes) }).html_safe %>

    NoteClipr.initialize(currentUser, notebooksData);
  });
</script>